{
  "version": 2,
  "builds": [
    { "src": "api/lineup.py", "use": "@vercel/python" },
    { "src": "web/**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/api/lineup",
      "methods": ["OPTIONS"],
      "dest": "/api/lineup.py",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    { "src": "/api/lineup", "dest": "/api/lineup.py" },
    { "src": "/(.*)", "dest": "/web/$1" }
  ]
}

